public class Person {
    private String name;
    private int age;
    private String gender;
    public Person(String name, int age, String gender) {
        this.name = name;
        this.age = age;
        this.gender = gender;
    }
    public void introduce() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("Gender: " + gender);
        System.out.println(); 
    }
    public static void main(String[] args) {
        Person person1 = new Person("janavi gowda", 21, "Female");
        Person person2 = new Person("Rakshitha", 17, "Female");
        System.out.println("Introducing Person 1:");
        person1.introduce();
        System.out.println("Introducing Person 2:");
        person2.introduce();
  }
}
